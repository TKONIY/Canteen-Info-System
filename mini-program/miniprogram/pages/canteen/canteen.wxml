<!--miniprogram/pages/canteen/canteen.wxml-->
<view class="container">
  <view class="guide">
   
    <text class="headline">{{canteenlist[mid-1].name}}</text>
    <text class="p">当前人流量</text>
    <text class="q">{{canteenlist[mid-1].people}}</text>
    
  </view>
  <button catchtap="f2" class="line" hover-class="line_hover">人流量变化线图</button>
  <text class="p">菜品一览</text>
  <text class="m">点击可查看文字评论</text>
  <view class="list1">
   
      <view class="uploader" wx:for="{{datalist}}" bindtap='f1' data-dish-id="{{item.id}}" >
        <view class="uploader-text1"> <text>{{item.name}}</text></view>
        <view class="uploader-text2"> <text>价格：{{item.price}}    得分：{{item.score}}  </text></view>
      <image class='.image1' src="{{item.image_url}}"></image>
      </view>

 


</view>


</view>
